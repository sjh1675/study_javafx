<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<HBox spacing="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bundle.BundleController">
   <children>
      <Button fx:id="btnAccept" onAction="#handleAction" text="%btn_accept" />
      <Button fx:id="btnReload" text="%btn_reload" />
      <Button fx:id="btnCancel" text="%btn_cancel" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
